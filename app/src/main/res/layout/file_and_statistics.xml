<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:id="@+id/friendWhiteBoxViewTwo"
        android:layout_width="360dp"
        android:layout_height="220dp"
        android:background="@drawable/notifications_textbox"
        android:elevation="15dp"
        android:layout_marginVertical="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView96"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:translationZ="16sp"
        app:layout_constraintStart_toStartOf="@+id/friendWhiteBoxViewTwo"
        app:layout_constraintTop_toBottomOf="@+id/textView97"
        app:srcCompat="@drawable/location_icon" />

    <ImageView
        android:id="@+id/imageView97"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="13dp"
        android:translationZ="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/friendWhiteBoxViewTwo"
        app:layout_constraintStart_toStartOf="@+id/friendWhiteBoxViewTwo"
        app:layout_constraintTop_toBottomOf="@+id/imageView96"
        app:layout_constraintVertical_bias="0.389"
        app:srcCompat="@drawable/home_file_statistics_icon" />

    <ImageView
        android:id="@+id/imageView98"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:translationZ="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView93"
        app:layout_constraintTop_toBottomOf="@+id/textView97"
        app:srcCompat="@drawable/entry_screen_chart_icon" />

    <ImageView
        android:id="@+id/imageView99"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="23dp"
        android:translationZ="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/friendWhiteBoxViewTwo"
        app:layout_constraintStart_toEndOf="@+id/textView98"
        app:layout_constraintTop_toBottomOf="@+id/imageView98"
        app:layout_constraintVertical_bias="0.237"
        app:srcCompat="@drawable/delete_file_icon" />

    <TextView
        android:id="@+id/textView94"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="36dp"
        android:fontFamily="sans-serif-medium"
        android:text="리포트 불러오기"
        android:textColor="#570861"
        android:textSize="15sp"
        android:translationZ="16sp"
        app:layout_constraintEnd_toEndOf="@+id/friendWhiteBoxViewTwo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView98"
        app:layout_constraintTop_toBottomOf="@+id/textView97" />

    <TextView
        android:id="@+id/textView95"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:fontFamily="sans-serif-medium"
        android:text="파일 삭제까지 n달 남음"
        android:textColor="#570861"
        android:textSize="14sp"
        android:translationZ="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/friendWhiteBoxViewTwo"
        app:layout_constraintEnd_toEndOf="@+id/friendWhiteBoxViewTwo"
        app:layout_constraintHorizontal_bias="0.12"
        app:layout_constraintStart_toEndOf="@+id/imageView99"
        app:layout_constraintTop_toBottomOf="@+id/imageView98"
        app:layout_constraintVertical_bias="0.166" />

    <TextView
        android:id="@+id/textView93"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        android:fontFamily="sans-serif-medium"
        android:maxWidth="115dp"
        android:text="주소"
        android:textColor="#570861"
        android:textSize="15sp"
        android:translationZ="16sp"
        app:layout_constraintStart_toEndOf="@+id/imageView96"
        app:layout_constraintTop_toBottomOf="@+id/textView97" />

    <TextView
        android:id="@+id/textView98"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="25dp"
        android:fontFamily="sans-serif-medium"
        android:text="녹음 파일 저장됨"
        android:textColor="#570861"
        android:textSize="15sp"
        android:translationZ="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/friendWhiteBoxViewTwo"
        app:layout_constraintStart_toEndOf="@+id/imageView97"
        app:layout_constraintTop_toBottomOf="@+id/imageView98"
        app:layout_constraintVertical_bias="0.245" />

    <TextView
        android:id="@+id/textView97"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="24dp"
        android:fontFamily="sans-serif-black"
        android:text="Y/M/C"
        android:textColor="#570861"
        android:textSize="25sp"
        android:translationZ="16sp"
        app:layout_constraintStart_toStartOf="@+id/friendWhiteBoxViewTwo"
        app:layout_constraintTop_toTopOf="@+id/friendWhiteBoxViewTwo" />

</androidx.constraintlayout.widget.ConstraintLayout>